<div class="Login-form">
  <h1>Login</h1>

  <%= form_tag '/login' do %>
  <p>
    Email: <%= text_field_tag :email %>
  </p>

  <p>
    Password: <%= password_field_tag :password %>
  </p>

  <p>
    <%= submit_tag "Submit" %>
  </p>

  <p>
    Not a Member? <%= link_to 'Sign Up', signup_path %>
  </p>


  <% end %>
</div>
