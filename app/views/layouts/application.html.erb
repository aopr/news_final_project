<!DOCTYPE html>
<html>
  <head>
    <title>NewsFinalProject</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav>
      <% if current_user %>
      <p>
        Hello, <%= current_user.u_name %> | <%= link_to 'Log Out', logout_path %>
      </p>
      <% end %>

    </nav>
    <%= yield %>
  </body>
</html>
