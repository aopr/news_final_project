<h1>ADMIN DASHBOARD</h1>

<h3>Last saved stories</h3>
<% Story.last(5).each do |s| %>
<div><%= s.body %></div>
<% end %>

<h3>New Rss stories</h3>
<% News_rss.last(5).each do |n| %>
<div><%= n.headline %> <%= n.url %></div>
<% end %>

<h3>New tweets</h3>
<% Soc_med.last(5).each do |t| %>
<div><%= t.text %></div>
<% end %>
